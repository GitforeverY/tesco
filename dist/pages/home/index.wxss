  margin-top: 28rpx;
}
.food-panel-item .group .group-content .group-content1,
.life-panel-item .group .group-content .group-content1,
.various-panel-item .group .group-content .group-content1 {
  width: 23%;
  height: 200rpx;
  margin: 0 7rpx;
  background: lawngreen;
  float: left;
}
.like-panel-item {
  margin: 10px;
  padding: 5px 10px;
}
.like-panel-item .like-title {
  width: 50%;
  margin: 0 auto;
}
.like-panel-item .like-title text {
  float: left;
  font-size: 32rpx;
  height: 32rpx;
  line-height: 32rpx;
}
.like-panel-item .like-title image {
