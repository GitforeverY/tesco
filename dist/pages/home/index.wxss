}
.like-panel-item .like-group-content {
  padding-top: 18rpx;
}
.like-panel-item .like-group-content .like-group-content-left:after {
  display: block;
  content: '';
  clear: both;
  height: 0;
}
.like-panel-item .like-group-content .like-group-content-left {
  margin-top: 18rpx;
  width: 49%;
  height: 530rpx;
  float: left;
  background: pink;
}
.like-panel-item .like-group-content .like-group-content-right {
  margin-top: 18rpx;
  width: 49%;
  height: 530rpx;
  float: right;
  background: pink;
}
